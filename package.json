{
  "name": "richardscarrott.co.uk",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run serve",
    "build": "node ./tasks/build",
    "serve": "node -r ./server/environment ./server",
    "test": "node --harmony_proxies ./node_modules/.bin/jest",
    "test:debug": "node-debug --harmony_proxies ./node_modules/.bin/jest",
    "test:watch": "node --harmony_proxies ./node_modules/.bin/jest --watch",
    "test:functional": "nightwatch --config ./test/functional/nightwatch.conf.js",
    "test:functional:remote": "nightwatch --env saucelabs --config ./test/functional/nightwatch.conf.js"
  },
  "engines": {
    "node": "4.4.4",
    "npm": "2.15.5"
  },
  "jest": {
    "automock": true,
    "modulePaths": [
      "<rootDir>/src"
    ],
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testRegex": ".*\\.test\\.js$",
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setup.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/react-helmet/",
      "<rootDir>/node_modules/react-redux/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/redux/",
      "<rootDir>/node_modules/redux-thunk/",
      "<rootDir>/node_modules/redux-mock-store/",
      "<rootDir>/node_modules/flux-standard-action/",
      "<rootDir>/node_modules/ramda/",
      "<rootDir>/node_modules/ramda-fantasy/",
      "<rootDir>/node_modules/normalizr/node_modules/lodash/",
      "<rootDir>/node_modules/identity-obj-proxy/"
    ],
    "moduleNameMapper": {
      "^.+factory\\.css$": "<rootDir>/test/unit/mocks/factoryStyleMock.js",
      "^.+\\.css$": "<rootDir>/test/unit/mocks/styleMock.js",
      "^.+\\.(jpe?g|png|gif|svg|woff|ttf|eot)$": "<rootDir>/test/unit/mocks/fileMock.js"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "60frames-coding-standards": "^1.0.7",
    "async": "^1.5.2",
    "autoprefixer": "^6.3.6",
