    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "enzyme": "^2.3.0",
    "errorhandler": "^1.4.3",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flux-standard-action": "^0.6.1",
    "friendly-errors-webpack-plugin": "^1.1.0",
    "fs-extra": "^0.26.7",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.6.5",
    "jest-cli": "^16.0.1",
